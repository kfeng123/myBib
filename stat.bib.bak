@Article{Li2016192,
  author    = {Yang Li and Zhaojun Wang and Changliang Zou},
  title     = {A simpler spatial-sign-based two-sample test for high-dimensional data},
  journal   = {Journal of Multivariate Analysis},
  year      = {2016},
  volume    = {149},
  pages     = {192 - 198},
  issn      = {0047-259X},
  abstract  = {Abstract This article concerns the tests for the equality of two location parameters when the data dimension is larger than the sample size. Existing spatial-sign-based procedures are not robust with respect to high dimensionality, producing tests with the type-I error rates that are much larger than the nominal levels. We develop a correction that makes the sign-based tests applicable for high-dimensional data, allowing the dimensionality to increase as the square of the sample size. We show that the proposed test statistic is asymptotically normal under elliptical distributions and demonstrate that it has good size and power in a wide range of settings by simulation. },
  doi       = {http://dx.doi.org/10.1016/j.jmva.2016.04.004},
